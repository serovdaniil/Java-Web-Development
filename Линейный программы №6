package com.company;

public class Main {

    public static void main(String[] args) {
       int x=3;
       int y=-2;
        if (((x>= -4)  && (x<= 4) && (y<= 0) && (y>= -3))||
                ((x>= -2)  && (x<= 2) && (y<= 4) && (y>= 0)))
        System.out.println("true");
        else
            System.out.println("false");
    }
}
